{
  "name": "@temporal-contract/worker-boxed",
  "version": "0.0.2",
  "description": "Type-safe Temporal worker with boxed Result/Future pattern for activities",
  "type": "module",
  "exports": {
    "./activity": {
      "import": {
        "types": "./dist/activity.d.mts",
        "default": "./dist/activity.mjs"
      },
      "require": {
        "types": "./dist/activity.d.cts",
        "default": "./dist/activity.cjs"
      }
    },
    "./workflow": {
      "import": {
        "types": "./dist/workflow.d.mts",
        "default": "./dist/workflow.mjs"
      },
      "require": {
        "types": "./dist/workflow.d.cts",
        "default": "./dist/workflow.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown src/activity.ts src/workflow.ts --format cjs,esm --dts --clean",
    "typecheck": "tsc --noEmit",
    "dev": "tsdown src/activity.ts src/workflow.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "temporal",
    "workflow",
    "contract",
    "type-safe",
    "boxed",
    "result",
    "future"
  ],
  "dependencies": {
    "@swan-io/boxed": "catalog:",
    "@temporal-contract/contract": "workspace:*",
    "@temporal-contract/worker": "workspace:*",
    "@standard-schema/spec": "catalog:"
  },
  "devDependencies": {
    "@temporal-contract/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "zod": "catalog:"
  },
  "peerDependencies": {
    "@temporalio/workflow": ">=1.13.0 <2.0.0"
  }
}
